{
  "routes": [
    {
      "route": "/callback",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/login",
      "serve": "/index.html", 
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}
tsconfig*.json
vite.config.ts
tailwind.config.js
postcss.config.js
eslint.config.js
jest.config.js

# Test files
**/*.test.*
**/*.spec.*
__tests__/
coverage/

# Package files
package-lock.json
yarn.lock
pnpm-lock.yaml

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db